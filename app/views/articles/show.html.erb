<% title "Article" %>

<p>
  <strong>Headline:</strong>
  <%=h @article.headline %>
</p>
<p>
  <strong>Content:</strong>
  <%=h @article.content %>
</p>

<p>
  <%= link_to "Edit", edit_article_path(@article) %> |
  <%= link_to "Destroy", @article, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", articles_path %>
</p>
